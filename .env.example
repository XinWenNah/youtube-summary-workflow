# Gemini API key
GEMINI_API_KEY=YOUR_GEMINI_API_KEY

# Google Sheets
SHEET_ID=YOUR_SHEET_ID
SHEET_URL=YOUR_GOOGLE_SHEET_URL
SHEET_NAME=YOUR_GOOGLE_SHEET_NAME

# Sheet GID (optional)
SHEET_GID=YOUR_GOOGLE_SHEET_GID
SHEET_GID_URL=YOUR_GOOGLE_SHEET_GID_URL
SHEET_GID_NAME=YOUR_GOOGLE_SHEET_GID_NAME

# Google Sheets OAuth credential
GOOGLE_SHEETS_CRED_ID=YOUR_GOOGLE_SHEETS_CRED_ID
GOOGLE_SHEETS_CRED_NAME=YOUR_GOOGLE_SHEETS_CRED_NAME

# Google Gemini (PaLM) credential
GOOGLE_PALM_CRED_ID=YOUR_GOOGLE_PALM_CRED_ID
GOOGLE_PALM_CRED_NAME=YOUR_GOOGLE_PALM_CRED_NAME

# Email addresses
SENDER_EMAIL=example.sender@gmail.com
RECEIVER_EMAIL=example.receiver@gmail.com

# SMTP credential
SMTP_ID=YOUR_SMTP_ID
SMTP_NAME=YOUR_SMTP_NAME